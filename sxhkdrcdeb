# Reload functions
super + Escape
	"$XDG_CONFIG_HOME"/sxhkd/functions

# brightness down
XF86MonBrightnessDown
	xbacklight -dec 5

# brightness up
XF86MonBrightnessUp
 	xbacklight -inc 5

# brightness down
super + XF86MonBrightnessDown
 	xbacklight -dec 1

# brightness up
super + XF86MonBrightnessUp
	xbacklight -inc 1
